@using System.Collections
@using Kendo.Mvc.UI

@(Html.Kendo().DropDownListFor(m => m)
                .Name("UserRowid")
                .DataValueField("UserRowid")
                .DataTextField("UserName")
                .SelectedIndex(0)
                .DataSource(ds => ds
                    .Read(read => read.Action("GetDoctorByProIdentity", "ZMCMSSysGetData"))
                )
                .Filter("contains")
    )

